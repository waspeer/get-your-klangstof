{
  "name": "get-your-klangstof",
  "version": "0.0.0",
  "main": "dist/index.js",
  "license": "MIT",
  "scripts": {
    "code:check:rules": "eslint --print-config .eslintrc.js | eslint-config-prettier-check",
    "code:check": "yarn code:lint; yarn code:format --check",
    "code:clean": "yarn code:lint --fix; yarn code:format --write",
    "code:format": "prettier './src/**/**.{js,jsx,ts,tsx}'",
    "code:lint": "eslint --ext .js,.jsx,.ts,.tsx \"src/\"",
    "dev": "ts-node-dev src/index.ts --respawn",
    "test": "jest"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@types/jest": "^25.1.3",
    "eslint-import-resolver-typescript": "^2.0.0",
    "husky": "^4.2.3",
    "jest": "^25.1.0",
    "poetic": "^1.4.1",
    "ts-jest": "^25.2.1",
    "ts-node-dev": "^1.0.0-pre.44"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "dependencies": {
    "@hapi/h2o2": "^9.0.1",
    "@hapi/hapi": "^19.1.1",
    "@types/hapi__hapi": "^19.0.2",
    "colors": "^1.4.0",
    "module-alias": "^2.2.2"
  },
  "_moduleAliases": {
    "#src": "src"
  }
}
